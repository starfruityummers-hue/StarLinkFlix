            { show: "South Park", season: "South Park Season 26", id: "1DieIk5axopu2BfHbg_GMIHZlvVIa-X7c", poster: "https://m.media-amazon.com/images/I/81+8sK3K2gL._AC_UL320_.jpg", type: "folder" },
            { show: "South Park", season: "South Park Season 27", id: "1qujbNl6HiE0SKMMsoJQGErqzGyeNelo4", poster: "https://static.wikia.nocookie.net/southparkfanon/images/6/6d/South_Park_Season_27_Fanmade_Poster.png/revision/latest?cb=20240217070609", type: "folder" }
        ];

        const familyGuyGrid = document.getElementById('family-guy-seasons');
        const squidGameGrid = document.getElementById('squid-game-seasons');
        const regularShowGrid = document.getElementById('regular-show-seasons');
        const simpsonsGrid = document.getElementById('simpsons-seasons');
        const spongebobGrid = document.getElementById('spongebob-seasons');
        const sonicXGrid = document.getElementById('sonic-x-seasons');
        const southParkGrid = document.getElementById('south-park-seasons');
        const moviesGrid = document.getElementById('movies');
        const episodesSection = document.getElementById('episodes-section');
        const seasonTitle = document.getElementById('season-title');
        const folderIframe = document.getElementById('folder-iframe');
        const errorMessage = document.getElementById('error-message');
        const loadingMessage = document.getElementById('loading-message');

        function loadSeasons() {
            console.log('Loading seasons and movies...');
            // Clear grids to prevent duplicates
            familyGuyGrid.innerHTML = '';
            squidGameGrid.innerHTML = '';
            regularShowGrid.innerHTML = '';
            simpsonsGrid.innerHTML = '';
            spongebobGrid.innerHTML = '';
            sonicXGrid.innerHTML = '';
            southParkGrid.innerHTML = '';
            moviesGrid.innerHTML = '';

            // Load Family Guy seasons
            const familyGuySeasons = data.filter(item => item.show === "Family Guy");
            console.log(`Found ${familyGuySeasons.length} Family Guy seasons`);
            familyGuySeasons.forEach((seasonData) => {
                const card = document.createElement('div');
                card.className = 'season-card';
                card.innerHTML = `
                    <img src="${seasonData.poster}" alt="${seasonData.season}" id="${seasonData.season.toLowerCase().replace(/\s+/g, '-')}" onerror="this.src='https://via.placeholder.com/200x300?text=${encodeURIComponent(seasonData.season)}';">
                    <h3>${seasonData.season.replace("Family Guy ", "")}</h3>
                `;
                card.addEventListener('click', () => {
                    console.log(`Clicked ${seasonData.season} (ID: ${seasonData.id})`);
                    loadSeasonFolder(seasonData);
                });
                familyGuyGrid.appendChild(card);
            });

            // Load Squid Game seasons
            const squidGameSeasons = data.filter(item => item.show === "Squid Game");
            console.log(`Found ${squidGameSeasons.length} Squid Game seasons`);
            squidGameSeasons.forEach((seasonData) => {
                const card = document.createElement('div');
                card.className = 'season-card';
                card.innerHTML = `
                    <img src="${seasonData.poster}" alt="${seasonData.season}" id="${seasonData.season.toLowerCase().replace(/\s+/g, '-')}" onerror="this.src='https://via.placeholder.com/200x300?text=${encodeURIComponent(seasonData.season)}';">
                    <h3>${seasonData.season.replace("Squid Game ", "")}</h3>
                `;
                card.addEventListener('click', () => {
                    console.log(`Clicked ${seasonData.season} (ID: ${seasonData.id})`);
                    loadSeasonFolder(seasonData);
                });
                squidGameGrid.appendChild(card);
            });

            // Load Regular Show seasons
            const regularShowSeasons = data.filter(item => item.show === "Regular Show");
            console.log(`Found ${regularShowSeasons.length} Regular Show seasons`);
            regularShowSeasons.forEach((seasonData) => {
                const card = document.createElement('div');
                card.className = 'season-card';
                card.innerHTML = `
                    <img src="${seasonData.poster}" alt="${seasonData.season}" id="${seasonData.season.toLowerCase().replace(/\s+/g, '-')}" onerror="this.src='https://via.placeholder.com/200x300?text=${encodeURIComponent(seasonData.season)}';">
                    <h3>${seasonData.season.replace("Regular Show ", "")}</h3>
                `;
                card.addEventListener('click', () => {
                    console.log(`Clicked ${seasonData.season} (ID: ${seasonData.id})`);
                    loadSeasonFolder(seasonData);
                });
                regularShowGrid.appendChild(card);
            });

            // Load The Simpsons seasons
            const simpsonsSeasons = data.filter(item => item.show === "The Simpsons");
            console.log(`Found ${simpsonsSeasons.length} Simpsons seasons`);
            simpsonsSeasons.forEach((seasonData) => {
                const card = document.createElement('div');
                card.className = 'season-card';
                card.innerHTML = `
                    <img src="${seasonData.poster}" alt="${seasonData.season}" id="${seasonData.season.toLowerCase().replace(/\s+/g, '-')}" onerror="this.src='https://via.placeholder.com/200x300?text=${encodeURIComponent(seasonData.season)}';">
                    <h3>${seasonData.season.replace("The Simpsons ", "")}</h3>
                `;
                card.addEventListener('click', () => {
                    console.log(`Clicked ${seasonData.season} (ID: ${seasonData.id})`);
                    loadSeasonFolder(seasonData);
                });
                simpsonsGrid.appendChild(card);
            });

            // Load SpongeBob SquarePants seasons/specials
            let spongebobSeasons = data.filter(item => item.show === "SpongeBob SquarePants");
            console.log(`Found ${spongebobSeasons.length} SpongeBob seasons/specials`);
            // Sort: Shorts first, Rare Finds second, then Seasons 1-16
            spongebobSeasons.sort((a, b) => {
                const getSortKey = (season) => {
                    if (season.includes("Shorts")) return -2;
                    if (season.includes("Rare Finds")) return -1;
                    const num = parseInt(season.match(/Season (\d+)/)?.[1] || 100);
                    return num;
                };
                return getSortKey(a.season) - getSortKey(b.season);
            });
            spongebobSeasons.forEach((seasonData) => {
                const card = document.createElement('div');
                card.className = 'season-card';
                card.innerHTML = `
                    <img src="${seasonData.poster}" alt="${seasonData.season}" id="${seasonData.season.toLowerCase().replace(/\s+/g, '-')}" onerror="this.src='https://via.placeholder.com/200x300?text=${encodeURIComponent(seasonData.season)}';">
                    <h3>${seasonData.season.replace("SpongeBob SquarePants ", "")}</h3>
                `;
                card.addEventListener('click', () => {
                    console.log(`Clicked ${seasonData.season} (ID: ${seasonData.id})`);
                    loadSeasonFolder(seasonData);
                });
                spongebobGrid.appendChild(card);
            });

            // Load Sonic X seasons/specials
            let sonicXSeasons = data.filter(item => item.show === "Sonic X");
            console.log(`Found ${sonicXSeasons.length} Sonic X seasons/specials`);
            // Sort: Seasons 1-3 first, Special Features last
            sonicXSeasons.sort((a, b) => {
                const getSortKey = (season) => {
                    if (season.includes("Special Features")) return 100;
                    const num = parseInt(season.match(/Season (\d+)/)?.[1] || 0);
                    return num;
                };
                return getSortKey(a.season) - getSortKey(b.season);
            });
            sonicXSeasons.forEach((seasonData) => {
                const card = document.createElement('div');
                card.className = 'season-card';
                card.innerHTML = `
                    <img src="${seasonData.poster}" alt="${seasonData.season}" id="${seasonData.season.toLowerCase().replace(/\s+/g, '-')}" onerror="this.src='https://via.placeholder.com/200x300?text=${encodeURIComponent(seasonData.season)}';">
                    <h3>${seasonData.season.replace("Sonic X ", "")}</h3>
                `;
                card.addEventListener('click', () => {
                    console.log(`Clicked ${seasonData.season} (ID: ${seasonData.id})`);
                    loadSeasonFolder(seasonData);
                });
                sonicXGrid.appendChild(card);
            });

            // Load South Park seasons/specials
            let southParkSeasons = data.filter(item => item.show === "South Park");
            console.log(`Found ${southParkSeasons.length} South Park seasons/specials`);
            // Sort: Season 0 first, then Seasons 1-27
            southParkSeasons.sort((a, b) => {
                const getSortKey = (season) => {
                    if (season.includes("Season 0")) return -1;
                    const num = parseInt(season.match(/Season (\d+)/)?.[1] || 100);
                    return num;
                };
                return getSortKey(a.season) - getSortKey(b.season);
            });
            southParkSeasons.forEach((seasonData) => {
                const card = document.createElement('div');
                card.className = 'season-card';
                card.innerHTML = `
                    <img src="${seasonData.poster}" alt="${seasonData.season}" id="${seasonData.season.toLowerCase().replace(/\s+/g, '-')}" onerror="this.src='https://via.placeholder.com/200x300?text=${encodeURIComponent(seasonData.season)}';">
                    <h3>${seasonData.season.replace("South Park ", "")}</h3>
                `;
                card.addEventListener('click', () => {
                    console.log(`Clicked ${seasonData.season} (ID: ${seasonData.id})`);
                    loadSeasonFolder(seasonData);
                });
                southParkGrid.appendChild(card);
            });

            // Load Movies
            const movies = data.filter(item => item.show === "Movies");
            console.log(`Found ${movies.length} Movies`);
            movies.forEach((seasonData) => {
                const card = document.createElement('div');
                card.className = 'season-card';
                card.innerHTML = `
                    <img src="${seasonData.poster}" alt="${seasonData.season}" id="${seasonData.season.toLowerCase().replace(/\s+/g, '-')}" onerror="this.src='https://via.placeholder.com/200x300?text=${encodeURIComponent(seasonData.season)}';">
                    <h3>${seasonData.season}</h3>
                `;
                card.addEventListener('click', () => {
                    console.log(`Clicked ${seasonData.season} (ID: ${seasonData.id})`);
                    loadSeasonFolder(seasonData);
                });
                moviesGrid.appendChild(card);
            });

            // Check if any cards were loaded
            if (familyGuySeasons.length === 0 && squidGameSeasons.length === 0 && regularShowSeasons.length === 0 && simpsonsSeasons.length === 0 && spongebobSeasons.length === 0 && sonicXSeasons.length === 0 && southParkSeasons.length === 0 && movies.length === 0) {
                console.error('No seasons or movies loaded!');
                alert('Error: No seasons or movies loaded. Please check the data array.');
            } else {
                console.log('All cards loaded successfully.');
            }
        }

        function loadSeasonFolder(seasonData) {
            console.log(`Attempting to load ${seasonData.season} with ID: ${seasonData.id}, Type: ${seasonData.type}`);
            seasonTitle.textContent = seasonData.season;
            errorMessage.style.display = 'none';
            loadingMessage.style.display = 'block';
            folderIframe.style.display = 'none';
            folderIframe.src = ''; // Reset iframe
            const url = seasonData.type === "folder" 
                ? `https://drive.google.com/embeddedfolderview?id=${seasonData.id}#list`
                : `https://drive.google.com/file/d/${seasonData.id}/preview`;
            console.log(`Setting iframe src to: ${url}`);
            folderIframe.src = url;
            episodesSection.style.display = 'block';
            episodesSection.scrollIntoView({ behavior: 'smooth' });

            // Handle iframe load events
            folderIframe.onload = () => {
                console.log(`Iframe loaded successfully for ${seasonData.season}`);
                loadingMessage.style.display = 'none';
                folderIframe.style.display = 'block';
            };
            folderIframe.onerror = () => {
                console.error(`Iframe failed to load for ${seasonData.season}`);
                loadingMessage.style.display = 'none';
                folderIframe.style.display = 'none';
                errorMessage.style.display = 'block';
            };
        }

        // Initialize
        try {
            loadSeasons();
        } catch (error) {
            console.error('Error loading seasons:', error);
            alert('Error initializing the page. Check the console for details.');
        }
    </script>
</body>
</html>
